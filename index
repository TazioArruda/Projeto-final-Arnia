    content.innerHTML = conteudo
}
novoPaciente = async ( novo) =>{

    await fetch ('http://localhost:3000/novoCadastro',{
        method:'POST',
        headers:{
            'Accept': 'application/json, text/plain, */*', 
            'Content-Type': 'application/json'  


        },
        body: JSON.stringify(novo)
    })
    fecharModal()
    fecharModal2()
}


